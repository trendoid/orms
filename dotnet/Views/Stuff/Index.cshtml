@model IEnumerable<WebApplication.Models.Quote>
    
@{
    ViewData["Title"] = "Stuff Politicians Say";
}

<div class="row">
    <div class="col-md-3">
        <h2>Stuff Politicians Say</h2>
    </div>
    <div class="col-md-9">
        <h2>Stuff</h2>
        <ul>
            @foreach (var item in Model)
            {
                <li>@Html.DisplayFor(modelItem => item.Text)</li>
            }
        </ul>
    </div>
</div>
